<div class="container">
  <div class="row">
    <div class="col-sm">
      <h1>Here's yer characters</h1>
    </div>
  </div>

  <div class="row">
    <div class="col-sm">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>Character</th>
          </tr>
        </thead>
        <tbody>
          <% @charnames.each do |char| %>
            <tr>
              <td><a href="<%= char + "/show" %>"><%= char %></a></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
